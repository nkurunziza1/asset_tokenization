{
  "canisters": {
    "azle": {
      "type": "custom",
      "main": "src/index.ts",
      "candid": "src/index.did",
      "build": "npx azle azle",
      "wasm": ".azle/azle/azle.wasm",
      "gzip": true,
      "declarations": {
        "output": "declarations/dfx_generated/",

        "node_compatibility": true
      }
    }
  },
  "networks": {
    "local": {
      "bind": "localhost:5050",
      "type": "ephemeral"
    }
  }
}
